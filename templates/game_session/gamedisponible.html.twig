{% extends 'base.html.twig' %}

{% block body %}
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                {% block fos_user_content %}

                    <div class="col-sm-10">
                        <h2 class="media-heading"><strong>  ¡Puede jugar el cuestionario, sesion de juego: {{ game.id }} !</strong> </h2>
                        <h2 class="media-heading"><strong> Contraseña para acceder: {{ game.password }}</strong> </h2>
                    </div>

                    {% for user in users %}


                            <ul>
                                <div class="col-sm-10">
                                    <h4 class="media-heading"><strong>  <li> {{ user.username}} </li> </strong> </h4>
                                </div>
                            </ul>


                    {% endfor %}





                {% endblock %}
            </div>
        </div>
    </div>
{% endblock %}